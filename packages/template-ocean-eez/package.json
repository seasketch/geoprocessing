    "@turf/bbox": "7.0.0",
    "@turf/combine": "7.0.0",
    "@turf/flatten": "7.0.0",
    "@turf/invariant": "7.0.0",
    "@turf/kinks": "7.0.0",
    "@turf/simplify": "7.0.0",
    "geoblaze": "2.6.1",
    "geojson-antimeridian-cut": "^0.1.0",
    "react-i18next": "^14.1.1",
    "union-subdivided-polygons": "^0.9.1"
